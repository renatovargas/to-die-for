	Issn = {0924-6460},
	Journal = {Environmental and Resource Economics},
	Keywords = {Business and Economics},
	Number = {3},
	Pages = {291--308},
	Shorttitle = {Health Damage of Air Pollution},
	Title = {Health Damage of Air Pollution: An Estimate of a Dose--Response Relationship for the {Netherlands}},
